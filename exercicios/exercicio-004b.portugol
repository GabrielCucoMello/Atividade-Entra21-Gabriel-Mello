/*
    Crie uma função que imprima uma mensagem ao ser chamada.
*/

programa {
funcao vazio linha_vazia()
{
escreva("\n==============================\n\n")
}

funcao inicio() 
{ 
  real a, b, mult

escreva ("Informe o primeiro numero: ")
leia (a)

escreva ("Informe o segundo numero: ")
